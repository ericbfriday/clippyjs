{
  "name": "@clippyjs/storybook",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/storybook",
  "projectType": "application",
  "tags": ["type:app", "scope:storybook", "platform:web"],
  "targets": {
    "storybook": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn workspace @clippyjs/storybook storybook"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/storybook-static"],
      "options": {
        "command": "yarn workspace @clippyjs/storybook build-storybook"
      }
    }
  }
}
